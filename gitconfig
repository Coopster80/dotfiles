[user]
	name = Nolan Cooper
	email = nolancooper97@gmail.com
[credential]
	helper = cache
[core]
	editor = vim
	excludesfile = /home/barrelmaker/.gitignore
[color]
	ui = auto
[color "branch"]
	current = cyan
	local = yellow
	remote = red
[color "diff"]
	meta = yellow bold
	frag = cyan
	old = red
	new = green
	whitespace = white reverse
[color "status"]
	added = green
	changed = yellow
	untracked = red
	branch = cyan
[alias]
	amend = commit -a --amend
	cm = commit -m
	br = branch
	ba = branch -a
	st = status
	conf = config --global -e
	hist = log --all --graph --pretty=format:'%Cred%h%Creset -%C(auto)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	sum = log --pretty=format:'%Cred%h%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	destroy = reset --hard HEAD
	up = pull --rebase
[push]
	default = simple
